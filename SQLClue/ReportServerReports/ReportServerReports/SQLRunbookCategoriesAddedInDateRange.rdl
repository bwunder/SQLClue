  <Code>Public Function RatingColor (ByVal Rating as Integer) As String
                        Select Case Rating
                            Case 1
                                RatingColor = "Green"
                            Case 2
                                RatingColor = "Lime"
                            Case 3
                                RatingColor = "GreenYellow"
                            Case 4
                                RatingColor = "Yellow"
                            Case 5
                                RatingColor = "Orange"
                            Case 6
                                RatingColor = "Red"
                            Case Else
                                RatingColor = "White"
                        End Select
End Function</Code>
